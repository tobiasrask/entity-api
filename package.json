  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobiasrask/entity-api/issues"
  },
  "homepage": "https://github.com/tobiasrask/entity-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "mocha": "5.2.0"
  },
  "dependencies": {
    "domain-map": "0.0.17",
    "uuid": "3.3.2"
  }
