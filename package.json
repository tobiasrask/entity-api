    "lint": "./node_modules/eslint/bin/eslint.js --ignore-path .gitignore src/* test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobiasrask/entity-api.git"
  },
  "keywords": [
